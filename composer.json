{
  "name": "zendframework/zend-expressive",
  "description": "Start to develop PSR-7 middleware applications in PHP in a minute!",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "http",
    "psr-7",
    "middleware"
  ],
  "homepage": "https://github.com/zendframework/zend-expressive",
  "support": {
    "issues": "https://github.com/zendframework/zend-expressive/issues",
    "source": "https://github.com/zendframework/zend-expressive"
  },
  "require": {
    "php": ">=5.5",
    "psr/http-message": "~1.0",
    "zendframework/zend-stratigility": "~1.0",
    "zendframework/zend-diactoros": "~1.0",
    "aura/router" : "~2.2"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~4.7",
    "squizlabs/php_codesniffer": "^2.3.1",
    "nikic/fast-route" : "~0.6"
  },
  "suggest" : {
    "nikic/fast-route" : "If you want to use the FastRoute route adapter"
  },
  "autoload": {
    "psr-4": {
      "Zend\\Expressive\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZendTest\\Expressive\\": "test/"
    }
  }
}
